{
    "name": "goazurefunctions",
    "image": "mcr.microsoft.com/devcontainers/base:1.0.12-bookworm",
    "features": {
        "ghcr.io/devcontainers/features/go:1": {
            "version": "1.22.4"
        }
    },
    "postCreateCommand": "./install.sh",
    "customizations": {
